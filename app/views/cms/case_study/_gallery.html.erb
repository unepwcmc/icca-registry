<% if @cms_page.photos.any? %>
  <div id='v-app'>
    <flickity ref="flickity" class="gallery__body" :options="flickityOptions">
      <% @cms_page.photos.each do |photo| %>
        <img src="<%= url_for(photo.file) %>" class="gallery__photo"/>
      <% end %>
    </flickity>
  </div>
</div>
<% end %>
